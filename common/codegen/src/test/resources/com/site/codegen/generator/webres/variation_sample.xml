<resource-variations>
   <resource-variation for="js" value="${res.js.local.jquery_js}">
      <variation env="feature">${res.js.local.jquery_min_js}</variation>
      <variation env="staging">${res.js.local.jquery_min_js}</variation>
      <variation env="production">${res.js.local.jquery_min_js}</variation>
   </resource-variation>
   <resource-variation for="css" value="${res.css.local.test_css}">
      <variation browser="ie6">${res.css.local.test_ie6_css}</variation>
      <variation browser="ie7">${res.css.local.test_ie7_css}</variation>
      <variation browser="ie8">${res.css.local.test_ie8_css}</variation>
   </resource-variation>
   <resource-variation for="pics" value="url_prefix">
      <variation env="production" secure="true">https://securepics.ebaystatic.com/aw/pics/</variation>
      <variation env="production">http://pics.ebaystatic.com/aw/pics/</variation>
      <variation secure="true">https://securepics.qa.ebaystatic.com/aw/pics/</variation>
      <variation>http://pics.qa.ebaystatic.com/aw/pics/</variation>
   </resource-variation>
</resource-variations>
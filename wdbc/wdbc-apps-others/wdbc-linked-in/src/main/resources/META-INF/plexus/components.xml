<plexus>
	<components>
		<component>
			<role>com.site.wdbc.linkedin.DatabaseConnectionFactory</role>
			<implementation>com.site.wdbc.linkedin.DatabaseConnectionFactory</implementation>
		</component>
      <component>
         <role>com.site.wdbc.linkedin.DatabaseAccess</role>
         <implementation>com.site.wdbc.linkedin.DatabaseAccess</implementation>
         <requirements>
            <requirement>
               <role>com.site.wdbc.linkedin.DatabaseConnectionFactory</role>
            </requirement>
         </requirements>
      </component>
		<component>
			<role>com.site.wdbc.http.Session</role>
			<implementation>com.site.wdbc.http.impl.DefaultSession</implementation>
			<configuration>
				<headers>
					<header name="User-Agent">Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.1; SV1; Maxthon; .NET CLR 1.1.4322)</header>
				</headers>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.http.Processor</role>
         <role-hint>linked-in</role-hint>
			<implementation>com.site.wdbc.linkedin.LinkedInProcessor</implementation>
         <requirements>
            <requirement>
               <role>com.site.wdbc.linkedin.DatabaseAccess</role>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.http.Processor</role>
         <role-hint>progress</role-hint>
			<implementation>com.site.wdbc.linkedin.ProgressProcessor</implementation>
         <configuration>
            <level1>company-title:company</level1>
            <level2>company-title:title</level2>
         </configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.linkedin.DatabaseAccess</role>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.http.Flow</role>
			<role-hint>linked-in</role-hint>
			<implementation>com.site.wdbc.http.impl.DefaultFlow</implementation>
         <configuration>
            <handler>
               <page name="login"/>
               <handler query="company-title" interval="10">
                  <page name="search"/>
                  <handler query="page-links" handle-current-page="true" interval="10">
                     <page name="list"/>
                     <handler query="result-set" interval="6">
                        <page name="details"/>
                        <handler query="profile"/>
                        <handler query="jobs">
                           <processor name="linked-in"/>
                        </handler>
                     </handler>
                  </handler>
                  <processor name="progress"/>
               </handler>
            </handler>
         </configuration>
		</component>
		<component>
			<role>com.site.wdbc.http.Request</role>
			<role-hint>login</role-hint>
			<implementation>com.site.wdbc.http.impl.FormRequest</implementation>
			<configuration>
				<action>https://www.linkedin.com/secure/login</action>
				<method>POST</method>
				<inputs>
					<input name="session_key">qmwu2000@gmail.com</input>
					<input name="session_password">Iamfrankie</input>
					<input name="session_login">Sign In</input>
				</inputs>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.http.Request</role>
			<role-hint>search</role-hint>
			<implementation>com.site.wdbc.http.impl.FormRequest</implementation>
			<configuration>
				<action>http://www.linkedin.com/search</action>
				<method>POST</method>
				<inputs>
               <input name="keywords"></input>
               <input name="kwname"></input>
               <input name="title">${company-title:title}</input>
               <input name="currentTitle">currentTitle</input>
               <input name="company">${company-title:company}</input>
               <input name="currentCompany">currentCompany</input>
               <input name="searchLocationType">I</input>
               <input name="countryCode">us</input>
               <input name="postalCode"></input>
               <input name="industry">4</input>
               <input name="industry">6</input>
               <input name="proposalType">Y</input>
               <input name="newnessType">Y</input>
               <input name="sortCriteria">4</input>
               <input name="search">Search</input>
               <input name="searchOrigin">S</input>
				</inputs>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.http.Request</role>
			<role-hint>list</role-hint>
			<implementation>com.site.wdbc.http.impl.FormRequest</implementation>
			<configuration>
				<action>http://www.linkedin.com${page-links:link}</action>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.http.Request</role>
			<role-hint>details</role-hint>
			<implementation>com.site.wdbc.http.impl.FormRequest</implementation>
			<configuration>
				<action>https://www.linkedin.com${result-set:link}</action>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>state-title</role-hint>
			<implementation>com.site.wdbc.linkedin.StateTitleQuery</implementation>
			<configuration>
            <state>CA</state>
				<titles>
               <title>Vice President</title>
               <title>VP</title>
               <title>Director</title>
               <title>Manager</title>
            </titles>
			</configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.linkedin.DatabaseAccess</role>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>company-title</role-hint>
			<implementation>com.site.wdbc.linkedin.CompanyTitleQuery</implementation>
			<configuration>
            <companies>
               <company>Oracle</company>
               <company>Cisco</company>
               <company>IBM</company>
               <company>Intel</company>
               <company>HP</company>
               <company>Apple</company>
               <company>Microsoft</company>
               <company>Google</company>
               <company>Sun Microsystem</company>
               <company>Yahoo</company>
               <company>Amazon</company>
               <company>BEA</company>
            </companies>
				<titles>
               <title>Vice President</title>
               <title>Director</title>
               <title>Manager</title>
            </titles>
			</configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.linkedin.DatabaseAccess</role>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>page-links</role-hint>
			<implementation>com.site.wdbc.SelectQuery</implementation>
			<configuration>
				<paths>
					<path name="link">p[17].a[*].@href</path>
					<path name="num">p[17].a[*]</path>
				</paths>
			</configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.query.WdbcFilter</role>
			      <role-hint>page-links</role-hint>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.query.WdbcFilter</role>
			<role-hint>page-links</role-hint>
			<implementation>com.site.wdbc.linkedin.PageLinksFilter</implementation>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>result-set</role-hint>
			<implementation>com.site.wdbc.SelectQuery</implementation>
			<configuration>
				<paths>
					<path name="id">-</path>
					<path name="link">ul[*].li[*].h2.a.@href</path>
					<path name="given-name">ul[*].li[*].h2.a.span.span</path>
					<path name="family-name">ul[*].li[*].h2.a.span.span[2]</path>
				</paths>
			</configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.query.WdbcFilter</role>
			      <role-hint>result-set-database</role-hint>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.query.WdbcFilter</role>
			<role-hint>result-set-database</role-hint>
			<implementation>com.site.wdbc.linkedin.ResultSetDatabaseFilter</implementation>
         <requirements>
            <requirement>
               <role>com.site.wdbc.query.WdbcFilter</role>
               <role-hint>result-set</role-hint>
            </requirement>
            <requirement>
               <role>com.site.wdbc.linkedin.DatabaseAccess</role>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.query.WdbcFilter</role>
			<role-hint>result-set</role-hint>
			<implementation>com.site.wdbc.linkedin.ResultSetFilter</implementation>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>profile</role-hint>
			<implementation>com.site.wdbc.SelectQuery</implementation>
			<configuration>
				<paths>
					<path name="link">div[*].div[*].dl.dd[*].a.@href</path>
					<path name="profile">div[*].div[*].dl.dd[*].a.@title</path>
				</paths>
			</configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.query.WdbcFilter</role>
			      <role-hint>profile</role-hint>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.query.WdbcFilter</role>
			<role-hint>profile</role-hint>
			<implementation>com.site.wdbc.linkedin.ProfileFilter</implementation>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>jobs</role-hint>
			<implementation>com.site.wdbc.SelectQuery</implementation>
			<configuration>
				<paths>
					<path name="flag">div[*].div[*].div[*].div[*].div[*].@name</path>
					<path name="title">div[*].div[*].div[*].div[*].div[*].h3.a.*text</path>
					<path name="company">div[*].div[*].div[*].div[*].div[*].p.strong.a.*text</path>
					<path name="year">div[*].div[*].div[*].div[*].p[2]</path>
               <path name="from">-</path>
               <path name="to">-</path>
				</paths>
			</configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.query.WdbcFilter</role>
			      <role-hint>jobs</role-hint>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.query.WdbcFilter</role>
			<role-hint>jobs</role-hint>
			<implementation>com.site.wdbc.linkedin.JobsFilter</implementation>
		</component>
   
      <!-- us state -->
		<component>
			<role>com.site.wdbc.http.Processor</role>
         <role-hint>us-states</role-hint>
			<implementation>com.site.wdbc.state.StateProcessor</implementation>
         <requirements>
            <requirement>
               <role>com.site.wdbc.linkedin.DatabaseAccess</role>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.http.Flow</role>
			<role-hint>us-states</role-hint>
			<implementation>com.site.wdbc.http.impl.DefaultFlow</implementation>
         <configuration>
            <handler>
               <page name="us-home"/>
               <handler query="state-links">
                  <page name="us-state"/>
                  <handler query="state-zipcode">
                     <processor name="us-states"/>
                  </handler>
               </handler>
            </handler>
         </configuration>
		</component>
		<component>
			<role>com.site.wdbc.http.Request</role>
			<role-hint>us-home</role-hint>
			<implementation>com.site.wdbc.http.impl.FormRequest</implementation>
			<configuration>
				<action>http://www.555us.com/</action>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.http.Request</role>
			<role-hint>us-state</role-hint>
			<implementation>com.site.wdbc.http.impl.FormRequest</implementation>
			<configuration>
				<action>${state-links:link}</action>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>state-links</role-hint>
			<implementation>com.site.wdbc.SelectQuery</implementation>
			<configuration>
				<paths>
					<path name="link">table.tr[8].td.table.tr[*].td[*].font.a.@href</path>
					<path name="state">table.tr[8].td.table.tr[*].td[*].font.a.b</path>
				</paths>
			</configuration>
		</component>
		<component>
			<role>com.site.wdbc.WdbcQuery</role>
			<role-hint>state-zipcode</role-hint>
			<implementation>com.site.wdbc.SelectQuery</implementation>
			<configuration>
				<paths>
					<path name="zipcode">-</path>
					<path name="city">-</path>
					<path name="text">pre.font</path>
				</paths>
			</configuration>
         <requirements>
            <requirement>
               <role>com.site.wdbc.query.WdbcFilter</role>
			      <role-hint>state-zipcode</role-hint>
            </requirement>
         </requirements>
		</component>
		<component>
			<role>com.site.wdbc.query.WdbcFilter</role>
			<role-hint>state-zipcode</role-hint>
			<implementation>com.site.wdbc.state.StateZipCodeFilter</implementation>
		</component>   
	</components>
</plexus>

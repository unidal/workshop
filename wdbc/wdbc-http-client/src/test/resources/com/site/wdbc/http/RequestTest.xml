			</requirements>
		</component>
		<component>
			<role>com.site.wdbc.http.Request</role>
			<role-hint>advanced-search</role-hint>
			<implementation>com.site.wdbc.http.impl.FormRequest</implementation>
			<configuration>
				<action>http://www.linkedin.com/search</action>
				<method>POST</method>
				<inputs>
					<input name="keywords"></input>
					<input name="kwname"></input>

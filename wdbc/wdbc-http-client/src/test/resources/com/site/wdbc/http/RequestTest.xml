			<configuration>
				<action>https://www.linkedin.com/secure/login</action>
				<method>POST</method>
				<inputs>
					<input name="session_key">qmwu2000@gmail.com</input>
					<input name="session_password">Iamfrankie</input>
					<input name="session_login">Sign In</input>
				</inputs>
				<headers>
